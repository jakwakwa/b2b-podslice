{
    "name": "my-v0-project",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "install:legacy": "bun install",
        "install:fast": "bun install",
        "install:clean": "rm -rf node_modules pnpm-lock.yaml package-lock.json",
        "dev": "next dev --turbopack",
        "dev:legacy": "next dev",
        "build": "bunx prisma generate --no-engine && next build",
        "prisma:generate": "bunx prisma generate --no-engine",
        "prisma:push": "bunx prisma db push",
        "prisma:migrate": "npx prisma migrate dev",
        "lint": "biome lint .",
        "lint:fix": "biome lint --write .",
        "start": "next start",
        "seed": "tsx scripts/seed.mjs",
        "seed:personas": "tsx scripts/seed-test-users.ts",
        "inngest": "inngest dev",
        "format": "biome format --write .",
        "format:fix": "biome format --write . && biome lint --write .",
        "test:db:deploy": "bash -lc 'set -a; [ -f .env.test ] && source .env.test; set +a; DATABASE_URL=${TEST_POSTGRES_PRISMA_URL:-$TEST_DATABASE_URL} pnpm prisma migrate deploy'",
        "test": "vitest run",
        "test:watch": "vitest --watch",
        "test:ci": "vitest run --coverage",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:debug": "playwright test --debug",
        "test:all": "bun test && bun test:e2e",
        "durations:update": "tsx scripts/update-existing-durations.ts all",
        "durations:update:episodes": "tsx scripts/update-existing-durations.ts episodes",
        "durations:update:user": "tsx scripts/update-existing-durations.ts user-episodes"
    },
